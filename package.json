{
  "name": "ember-side-menu",
  "version": "0.0.15",
  "description": "Mobile friendly side menu component for Ember.js applications",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "ember try:each",
    "deploy": "ember github-pages:commit --message 'demo page release'",
    "deploy:test": "rm -rf dist/ && ember build && surge -p ./dist -d esm-test.surge.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsubik/ember-side-menu.git"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ember-cli-babel": "^6.6.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.6",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~3.0.2",
    "ember-cli-app-version": "^2.0.0",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-github-pages": "0.1.2",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-sass": "5.5.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-font-awesome": "^4.0.0-rc.2",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.0.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint": "^2.10.2",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-node": "^5.2.1",
    "loader.js": "^4.2.3"
  },
  "keywords": [
    "ember-addon",
    "side-menu",
    "sliding-menu",
    "menu",
    "drawer",
    "sliding",
    "toggle-menu",
    "mobile-menu",
    "drawer-menu",
    "burger-menu"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://tsubik.com/ember-side-menu"
  }
}
